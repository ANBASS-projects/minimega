# Simple tests
vm config net 2
vm config net A # should be 3
vlans
vm config net
vm config net B # should be 4
vlans
vm config net
vm config net A # should be 3
vlans
vm config net

# Manually add aliases
vlans add foo 10
vlans add foo 11
vlans add bar 12
vlans
vm config net foo
vm config net

# Tests with namespaces
namespace foo
vm config net A # should be 5
vlans
vm config net
namespace bar
vm config net A # should be 6
vlans
vm config net
vm config net //A # should be 3
vlans
vm config net
vm config net foo//A # should be 5
vlans
vm config net
clear namespace

# Specify ranges
namespace foo
vlans range 30 20
vlans range 20 30
vm config net B # should be 20
vlans
vm config net
